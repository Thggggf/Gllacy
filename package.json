{
    "name": "Gllacy",
    "version": "17.0.0",
    "private": true,
    "description": "Личный проект «Глейси» от HTML Academy",
    "devDependencies": {
        "@htmlacademy/editorconfig-cli": "1.x",
        "stylelint": "10.1.x",
        "stylelint-config-htmlacademy": "0.1.x"
    },
    "scripts": {
        "editorconfig": "editorconfig-cli",
        "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
        "test": "npm run editorconfig && npm run stylelint"
    },
    "browserslist": [
        "last 2 versions",
        "IE 11",
        "Firefox ESR"
    ],
    "editorconfig-cli": [
        "*.json",
        "*.js",
        "source/*.html",
        "source/js/**/*.js",
        "source/img/**/*.svg",
        "source/less/**/*.less"
    ],
    "engines": {
        "node": "8.16"
    },
    "dependencies": {
        
    }
}
